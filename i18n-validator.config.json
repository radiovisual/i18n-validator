{
	"defaultLocale": "en",
	"sourceFile": "en.json",
	"translationFiles": {
		"de": "de.json",
		"fr": "fr.json"
	},
	"pathToTranslatedFiles": "src/tests/fixtures/i18n",
	"rules": {
