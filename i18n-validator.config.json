{
	"defaultLocale": "en",
	"sourceFile": "en.json",
	"translationFiles": {
		"de": "de.json",
		"fr": "fr.json"
	},
	"pathToTranslatedFiles": "src/tests/fixtures/i18n",
	"rules": {
		"no-untranslated-messages": {
			"severity": "error",
			"ignoreKeys": ["ok", "pizza"]
		},
		"no-empty-messages": "error"
	},
	"dryRun": false,
	"enabled": true
}
